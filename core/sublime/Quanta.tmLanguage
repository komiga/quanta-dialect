				</dict>
				<key>4</key>
				<dict>
					<key>name</key>
					<string>quanta.tag.marker.begin</string>
				</dict>
			</dict>
			<key>end</key>
			<string>(\))?</string>
			<key>endCaptures</key>
			<dict>
				<key>1</key>
				<dict>
					<key>name</key>
					<string>quanta.tag.marker.end</string>
				</dict>
			</dict>
			<key>patterns</key>
			<array>
