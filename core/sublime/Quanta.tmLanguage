		<key>markers</key>
		<dict>
			<key>name</key>
			<string>quanta.marker</string>
			<key>match</key>
			<string>(?x)(
				G?~+ | G?~\^+ |
				\+ | \- | \* | / | % |
				\! | \^ | \&amp;\&amp;? | \|\|? | &lt;&lt; | &gt;&gt; |
				\!= | == | &lt;=? | &gt;=?
			)</string>
		</dict>

		<key>constants</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>match</key>
					<string>\b(null|true|false)\b</string>
					<key>name</key>
					<string>quanta.constant</string>
				</dict>
				<dict>
					<key>match</key>
					<string>(?x)
						\b(?:
							(0(?:x|X)[0-9a-fA-F]*)\b |
							((?:(?:[0-9]+\.?[0-9]*)|(?:\.[0-9]+))(?:(?:e|E)(?:\+|\-)?[0-9]+)?)(°?[[:alpha:]_\.](?:[[:alnum:]_\-\.]*[[:alnum:]_\.]+)?)?
						)
					</string>
					<key>captures</key>
					<dict>
						<key>1</key>
						<dict>
							<key>name</key>
							<string>quanta.value.number</string>
						</dict>
