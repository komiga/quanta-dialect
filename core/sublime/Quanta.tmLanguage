			<key>begin</key>
			<string>([[:alpha:]_\.](?:[[:alnum:]_\-\.]*[[:alnum:]_\.]+)?)((:)([[:alpha:]_\.](?:[[:alnum:]_\-\.]*[[:alnum:]_\.]+)?))*\s*(\{)</string>
